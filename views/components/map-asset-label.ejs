<%

  var mapAssetLabel = mapAssetLabel || {
    status: null,
    dev: false
  };

%>

<% if(mapAssetLabel.dev) { %>
  <div class="d-flex flex-row justify-content-around pt-5">
<% } %>

  <div class="map-asset-label px-2 <%- mapAssetLabel.status %>">
    <p class="mb-0">R-8162</p>
    <hr class="my-0" />
    <p class="mb-1">42%</p>

    <div class="progress">
      <div class="progress-bar w-50"
        role="progressbar"
        aria-valuenow="40"
        aria-valuemin="0"
        aria-valuemax="100"
      ></div>
    </div>
  </div>

  <% if(mapAssetLabel.dev) { %>
    <div class="map-asset-label px-2 warning">
      <p class="mb-0">R-8162</p>
      <hr class="my-0" />
      <p class="mb-1">42%</p>

      <div class="progress">
        <div class="progress-bar w-50"
          role="progressbar"
          aria-valuenow="40"
          aria-valuemin="0"
          aria-valuemax="100"
        ></div>
      </div>
    </div>

    <div class="map-asset-label px-2 danger">
      <p class="mb-0">R-8162</p>
      <hr class="my-0" />
      <p class="mb-1">42%</p>

      <div class="progress">
        <div class="progress-bar w-50"
          role="progressbar"
          aria-valuenow="40"
          aria-valuemin="0"
          aria-valuemax="100"
        ></div>
      </div>
    </div>
  <% } %>

<% if(mapAssetLabel.dev) { %>
  </div>
<% } %>
